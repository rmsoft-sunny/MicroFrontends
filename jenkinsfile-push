node {
     stage('Clone repository') {
         checkout scm
     }

     stage('Build image') {
         app = docker.build("teichae/jenkins")
     }
     stage('Push image') {
         docker.withRegistry('https://registry.hub.docker.com', 'f3854f9d-f479-4478-9461-990b5e07ff56') {
             app.push("${env.BUILD_NUMBER}")
             app.push("latest")
             app.push("jes@rmsoft.kr")
         }
     }
 }