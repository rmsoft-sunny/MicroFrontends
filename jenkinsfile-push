node {
     stage('Clone repository') {
         checkout scm
     }

     stage('Build image') {
         app = docker.build("eunseonjeong/clive-trans")
     }
     stage('Push image') {
                script {
                    sh 'docker compose up -d'
                }
     }
  
    stage('Build') {
            container('test') {
                sh "echo pipeline test"
            }
    
 }
