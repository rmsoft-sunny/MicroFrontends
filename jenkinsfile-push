node {
     stage('Clone repository') {
         checkout scm
     }

     stage('Build image') {
         app = docker.build("eunseonjeong/clive-trans")
     }
     stage('Push image') {
                script {
                    sh 'docker run -p 3000:3000 -d eunseonjeong/clive-trans '
                }
     }
 }

 //도커 run -> build 된 파일 실행